            },
            {
              "text": "~200,000 => ~400,000 voters",
              "color": "lightgrey"
            },
            {
              "text": "Working classes excluded",
              "color": "lightgrey"
            }
          ]
        },
        {
          "text": "Poor Law Amendment Act (1834)",
          "color": "darkslategray",
          "children": [
            {
              "text": "Previous system: Out-relief",
              "color": "darkslategray"
            },
            {
              "text": "New System: Workhouse with bad conditions.",
              "color": "darkslategray"
            },
            {
              "text": "Philosophy: Prevent Laziness",
              "color": "darkslategray"
            }
          ]
        },
        {
          "text": "Factory Act (1833)",
          "color": "darkslategray",
